<!DOCTYPE html>
<html>
<head>

    <title>Chris Nabors Works - @ViewBag.Title</title>
    <meta charset="utf-8" />

    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css" integrity="sha384-mzrmE5qonljUremFsqc01SB46JvROS7bZs3IO2EmfFsd15uHvIt+Y8vEf7N7fWAU" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/design.css" />
</head>


<body>
    <div class="container" id="container">
        <header class="header" id="header">
            <h2 class="h2" id="h2">Chris Nabors Works</h2>

            <div class="main-nav" id="main-nav">
                <ul class="main-nav ul">
                    <li class="main-nav li"> <a asp-controller="Home" asp-action="Index">HOME</a></li>
                    <li class="main-nav li"> <a asp-controller="Home" asp-action="About">ABOUT</a></li>
                    <li class="main-nav li"> <a asp-controller="Home" asp-action="ApplicationDevelopment">Application Development</a></li>
                    <li class="main-nav li"> <a asp-controller="Home" asp-action="VideoProductions">Video Productions</a></li>
                    <li class="main-nav li"> <a asp-controller="Home" asp-action="Contact">CONTACT</a></li>
                </ul>
            </div>
        </header>
        <div id="google_translate_element"></div>
        @RenderBody()

        <br />
        <br />
        <br />
        <br />
        <br />

        <footer class="footer" id="footer">
            <i>&copy; 2018 Digi Integrations LLC</i>
        </footer>
    </div>
</body>

</html>
@RenderSection("scripts", false)
<script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
<script src="~/js/site.js"></script>
<meta name="google-translate-customization" content="a648e4b2b9009c0f-ca96432aba1106ac-gb7380bc44e8f6cea-10" />
<script type="text/javascript" src="http://cdn.howcode.org/content/static/javascript/jquery.min.js"></script>
<script src="http://cdn.howcode.org/content/static/javascript/jquery.cookie.js"></script>
<script type="text/javascript">
    function googleTranslateElementInit() {
        new google.translate.TranslateElement(
        {
            pageLanguage: 'en', layout: google
                .translate.TranslateElement
                .InlineLayout.SIMPLE, multilanguagePage: true
        }, 'google_translate_element');
    }
</script>
<script type="text/javascript" src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>
<script type="text/javascript">
    $.cookie('googtrans', '/en/en')

</script>